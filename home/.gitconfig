[user]
  email = david.w.millar@gmail.com
  name  = David W Millar
[core]
  editor   = vim
  autocrlf = input
[color]
  ui = true
[alias]
  serve = !git daemon --reuseaddr --verbose --base-path=. --export-all ./.git
[credential]
  helper = osxkeychain
[github]
  user = david-w-millar
[hub]
  protocol = https
[push]
  default = simple
[filter "media"]
  clean = git-media-clean %f
  smudge = git-media-smudge %f
